CREATE TABLE LINEITEM (
L_ORDERKEY INT UNSIGNED,
L_PARTKEY INT UNSIGNED,
L_SUPPKEY INT UNSIGNED, 
L_LINENUMBER INT,
L_QUANTITY DECIMAL(10,0),
L_EXTENDEDPRICE DECIMAL(10,0),
L_DISCOUNT DECIMAL(10,0),
L_TAX DECIMAL(10,0),
L_RETURNFLAG CHAR(1),
L_LINESTATUS CHAR(1),
L_SHIPDATE DATE,
L_COMMITDATE DATE,
L_RECEIPTDATE DATE,
L_SHIPINSTRUCT CHAR(25),
L_SHIPMODE CHAR(10),
L_COMMENT VARCHAR(44),
PRIMARY KEY (L_ORDERKEY, L_LINENUMBER)
) ENGINE = InnoDB
;
